<!DOCTYPE html>
<html>
  
<head>
  <title>CareProcess</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  <div style="text-decoration: none">
  <%= link_to "Users", users_path %>
  <%= link_to "Home", home_path %>
  <%= link_to "BGC", home_doc_download_path %>
  <%= link_to "BGC download", home_bgc_download_path %>
</div>
</head>
<body>
  <div id="header">
    <div id="user_nav">
      <% if current_user %>
      Logged in as <%= current_user.emp_name.capitalize %>
      <% end %>
    </div>
  </div>
   <div id="content-body">    
  
    <%= yield %>
     
   </div>
  <div id="footer">
    <div id="user_nav">
      <% if current_user %>
     <%= link_to "Log out", log_out_path %>
      <% end %>
    </div>
  </div>


</body>
</html>
